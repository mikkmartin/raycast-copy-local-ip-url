{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "copy-local-ip-url",
  "title": "Copy local IP URL",
  "description": "Copies current tab localhost url as local IP url",
  "icon": "command-icon.png",
  "author": "mikkmartin",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Copy localhost as local IP URL",
      "description": "Template for running a script without any view",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.4",
    "run-applescript": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "~20.1.0",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
